                    OptionalConfiguration opt ;
    Color colo;
    colo.B = 0.0;
    colo.R = 0.0;
    colo.G = 0.0;
    opt.marginXSpacing = 7;
    opt.marginYSpacing = 9;
    opt.textColor = colo;
    

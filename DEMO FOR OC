    colo2.R = 1;
    colo2.G = 1;
    colo2.B = 1;
    opt.menuBackgroundColor = colo2;
    opt.menuCornerRadius = 6;
    opt.arrowSize = 6;
    opt.intervalSpacing = 15;
    opt.maskToBackground = NO;
    opt.shadowOfMenu = YES;
    opt.hasSeperatorLine = YES;
    
            [KxMenu showMenuInView:btn  fromRect:CGRectMake(0, 0, 120, 120) menuItems:self.datas withOptions:opt];
